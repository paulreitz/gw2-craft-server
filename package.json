{
  "name": "gw2-crafting-api",
  "version": "1.0.0",
  "description": "The server end of a utility to get the full tree and base material needed for crafting items in GW2.",
  "main": "server.js",
  "scripts": {
    "build:ci": "~/project/node_modules/typescript/bin/tsc",
    "build": "tsc",
    "build:watch": "./node_modules/typescript/bin/tsc -w",
    "tsc:version": "tsc -v",
    "postinstall": "tsc",
    "start": "node server.js"
  },
  "engines": {
    "node": "8.3.0",
    "npm": "5.3.0"
  },
  "author": "Paul Reitz",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "mssql": "^3.3.0",
    "promise": "^7.1.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/express": "^4.0.35",
    "@types/mssql": "^3.3.1",
    "@types/promise": "^7.1.30"
  }
}
